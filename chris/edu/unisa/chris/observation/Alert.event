package edu.unisa.chris.observation;
/**
 * This knowledge stage is achieved when the agent has recieved new data, ready for processing.
 * @author  Christos Sioutis
 */
public event Alert extends Event{
    public Object alertData;
    /** Creates a new instance of Alert */
    #posted as alert(Object newData) {
        alertData = newData;
    }
}
